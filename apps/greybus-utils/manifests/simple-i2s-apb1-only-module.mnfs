;
; Simple I2S Module Manifest for APB2
;

[manifest-header]
version-major = 0
version-minor = 1

[module-descriptor]
vendor = 0xffff
product = 0x0001
version = 1
vendor-string-id = 1
product-string-id = 2
unique-id = 0

; Module vendor string (id can't be 0)
[string-descriptor "1"]
id = 1
string = "Project Ara"

; Module product string (id can't be 0)
[string-descriptor "2"]
id = 2
string = "Simple I2S Module"

; I2S management protocol on CPort 0 (AP->APB1 Mgmt CPort)
[cport-descriptor "0"]
interface = 0
id = 0
protocol = 0xa

; I2S data protocol on CPort 1 (AP->APB1 Receiver CPort)
[cport-descriptor "1"]
interface = 0
id = 1
protocol = 0x11

; Interface 0
[interface-descriptor "0"]
id = 0

; Control Class
[class-descriptor "0"]
Class = 0x00
